curl https://download.nextcloud.com/server/releases/?F=0 | awk {' print $3'} | awk -F- {'print $2'} | awk -F. {'print $1"."$2"."$3'} | egrep "11.0|12.0|13.0|14.0" | sort -n | uniq > versions
for i in $(cat versions)
do echo $i
cp Dockerfile Dockerfile.$i
sed -i 's/^ARG NC_VER.*/ARG NC_VER='"${i}"'/' Dockerfile.$i
done
